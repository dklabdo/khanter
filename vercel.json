{
    "rewrites": [
      {
        "source": "/:path*",
        "destination": "/:path*"
      },
      {
        "source": "/admin",
        "destination": "/admin"
      }
    ],
    "headers": [
      {
        "source": "/(.*)",
        "headers": [
          {
            "key": "Cache-Control",
            "value": "public, max-age=3600"
          }
        ]
      }
    ]
  }